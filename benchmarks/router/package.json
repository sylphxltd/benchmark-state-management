{
  "name": "router-benchmark",
  "version": "1.0.0",
  "description": "Professional React router library benchmark suite using Vitest Bench",
  "type": "module",
  "scripts": {
    "benchmark": "vitest bench --run --outputJson=results/latest.json && node ../../scripts/save-dated-result.cjs results",
    "clean": "rm -rf results && mkdir -p results"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "@sylphx/zen-router": "^1.0.2",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "tsx": "^4.20.6",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
