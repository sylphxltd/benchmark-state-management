{
  "name": "state-management-benchmark",
  "version": "1.0.0",
  "description": "Professional state management benchmark suite using Vitest Bench",
  "type": "module",
  "scripts": {
    "benchmark": "vitest bench --run --outputJson=results/latest.json && node ../../scripts/save-dated-result.cjs results",
    "clean": "rm -rf results && mkdir -p results"
  },
  "dependencies": {
    "@preact/signals": "^2.3.2",
    "@reduxjs/toolkit": "^2.10.1",
    "@sylphx/zen": "^1.0.0",
    "jotai": "^2.15.1",
    "mobx": "^6.15.0",
    "recoil": "^0.7.7",
    "solid-js": "^1.9.10",
    "valtio": "2.2.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "tsx": "^4.20.6",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
